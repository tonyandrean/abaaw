<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!--Define tree of tree views-->
    <record id="hitung_jib_view_tree" model="ir.ui.view">
        <field name="name">hitung_jib.view.tree</field>
        <field name="model">hitung.jib</field>
        <field name="arch" type="xml">
            <tree string="Jumlah jurnal reputasi pertahun bagi jumlah dosen" create="false" edit="false" delete="false">
                <field name="tahun"></field>
                <field name="jumlah_record"></field>
                <field name="jumlah_dosen"></field>
                <field name="hasil"></field>
            </tree>
        </field>
    </record>

    <record id="action_hitung_rens" model="ir.actions.act_window">
        <field name="name">Hitung Jurnal Internasional Bereputasi</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hitung.jib</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Data Unavailable
            </p>
        </field>
    </record>

</odoo>
