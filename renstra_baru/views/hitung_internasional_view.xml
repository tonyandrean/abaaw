<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="hitung_ji_view_tree" model="ir.ui.view">
        <field name="name">hitung_ji.view.tree</field>
        <field name="model">hitung.ji</field>
        <field name="arch" type="xml">
            <tree string="Jumlah jurnal internasional pertahun bagi jumlah dosen" create="false" edit="false" delete="false">
                <field name="tahun"></field>
                <field name="jumlah_record"></field>
                <field name="jumlah_dosen"></field>
                <field name="hasil"></field>
            </tree>
        </field>
    </record>

    <record id="action_hitung_internasional" model="ir.actions.act_window">
        <field name="name">Hitung Jurnal Internasional</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hitung.ji</field>
        <field name="view_mode">tree</field>
        <field name="context">{}</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Data Unavailable
            </p>
        </field>
    </record>

</odoo>
